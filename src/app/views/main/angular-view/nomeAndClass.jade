.form-group
	input.form-control(type='text', name='taxonRecordName', ng-model='taxonNameResearch')
	button.btn.btn-default.btn-sm(ng-click='getTaxonInformation(taxonNameResearch)') Get information
	br
	h3 Taxon Record Name:
	hr
	div(ng-show='formData.taxonRecordName.scientificName.simple')
		div Scientific Name :{{formData.taxonRecordName.scientificName.simple}}
	div(ng-show='formData.taxonRecordName.scientificName.publishedln.simple')
		div Published ln :{{formData.taxonRecordName.scientificName.publishedln.simple}}
	hr
	h3 Synonyms:
	hr(noshade='')
	div(ng-repeat='synonymAtomized in formData.synonymsAtomized')
		div(ng-show='synonymAtomized.synonymName.simple')
			div Synonym Name :{{synonymAtomized.synonymName.simple}}
		div(ng-show='synonymAtomized.synonymStatus')
			div Synonym Status :{{synonymAtomized.synonymStatus}}
		hr
	div(ng-controller='CollapseDemoCtrl')
		button.btn.btn-default(ng-click='isCollapsed = !isCollapsed') Add Synonym
		hr
		div(collapse='isCollapsed')
			.form-group
				h4 Synonym Name:
				input(type='text', ng-model='synonmy.canonicalName')
				br
				h4 Synonym Status:
				input(type='text', ng-model='synonmy.synonymStatus')
				br
				h4 Published ln:
				.form-group
					h5 Identifier:
					input(type='text', ng-model='synonmy.publishedln.identifier')
					br
					h5 Datatype:
					input(type='text', ng-model='synonmy.publishedln.datatype')
					br
					h5 Source:
					input(type='text', ng-model='synonmy.publishedln.source')
				br
				h4 Ancillary Data:
				button.btn.btn-info.btn-sm(type='button', ng-click='addAncillaryData(synonmy.ancillaryData)')
					| Ancillary Data
					span.glyphicon.glyphicon-plus
				div(ng-repeat='ancillary in synonmy.ancillaryData')
					.form-group
						h4 License:
						input(type='text', ng-model='ancillary.license')
						hr
						h4 Rights Holder:
						input(type='text', ng-model='ancillary.rightsHolder')
						hr
						h4 Bibliographic Citation:
						input(type='text', ng-model='ancillary.bibliographicCitation')
						hr
						h4 Source:
						input(type='text', ng-model='ancillary.source')
						hr
						h4 Media URL:
						input(type='text', ng-model='ancillary.mediaURL')
						hr
						h4 References:
						div(ng-repeat='ref in ancillary.reference')
							h5 Identifier:
							input(type='text', ng-model='ref.identifier')
							br
							h5 Datatype:
							input(type='text', ng-model='ref.datatype')
							br
							h5 Source:
							input(type='text', ng-model='ref.source')
							hr
						button.btn.btn-info.btn-sm(type='button', ng-click='addReference(ancillary.reference)')
							| Add Reference
							span.glyphicon.glyphicon-plus
						hr
				br
				button.btn.btn-info.btn-sm(type='button', ng-click='addSynonymsAtomized(formData.synonymsAtomized,synonmy)') Accept
	br
	h3 Common Names:
	hr
	div(ng-repeat='commonNameAtomized in formData.commonNameAtomized')
		div(ng-show='commonNameAtomized.name')
			div Common Name :{{commonNameAtomized.name}}
		div(ng-show='commonNameAtomized.language')
			div Language :{{commonNameAtomized.language}}
		div(ng-show='commonNameAtomized.usedIn.distributionUnstructured')
			div Used In :{{commonNameAtomized.usedIn.distributionUnstructured}}
		div(ng-show='commonNameAtomized.usedBy')
			div used By :{{commonNameAtomized.usedBy}}
		hr
	div(ng-controller='CollapseDemoCtrl')
		button.btn.btn-default(ng-click='isCollapsed = !isCollapsed') Add Common Name
		hr
		div(collapse='isCollapsed')
			.form-group
				h4 Name:
				input(type='text', ng-model='commonName.name')
				br
				h4 Language:
				input(type='text', ng-model='commonName.language')
				br
				h4 Used In:
				input(type='text', ng-model='commonName.usedIn')
				br
				h4 Used By:
				input(type='text', ng-model='commonName.usedBy')
				br
				h4 Ancillary Data:
				button.btn.btn-info.btn-sm(type='button', ng-click='addAncillaryData(commonName.ancillaryData)')
					| Ancillary Data
					span.glyphicon.glyphicon-plus
				div(ng-repeat='ancillary in commonName.ancillaryData')
					.form-group
						h4 License:
						input(type='text', ng-model='ancillary.license')
						hr
						h4 Rights Holder:
						input(type='text', ng-model='ancillary.rightsHolder')
						hr
						h4 Bibliographic Citation:
						input(type='text', ng-model='ancillary.bibliographicCitation')
						hr
						h4 Source:
						input(type='text', ng-model='ancillary.source')
						hr
						h4 Media URL:
						input(type='text', ng-model='ancillary.mediaURL')
						hr
						h4 References:
						div(ng-repeat='ref in ancillary.reference')
							h5 Identifier:
							input(type='text', ng-model='ref.identifier')
							br
							h5 Datatype:
							input(type='text', ng-model='ref.datatype')
							br
							h5 Source:
							input(type='text', ng-model='ref.source')
							hr
						button.btn.btn-info.btn-sm(type='button', ng-click='addReference(ancillary.reference)')
							| Add Reference
							span.glyphicon.glyphicon-plus
						hr
				br
				button.btn.btn-info.btn-sm(type='button', ng-click='addCommonNamesAtomized(formData.commonNameAtomized,commonName)') Accept
	br
	h3 Hierarchies:
	hr
	div(ng-repeat='hierarchy in formData.hierarchy')
		div(ng-show='hierarchy.classification')
			div classification :{{hierarchy.classification}}
		div(ng-show='hierarchy.recommended')
			div recommended :{{hierarchy.recommended}}
		div(ng-show='hierarchy.kingdom')
			div kingdom:{{hierarchy.kingdom}}
		div(ng-show='hierarchy.phylum')
			div phylum :{{hierarchy.phylum}}
		div(ng-show='hierarchy.classHierarchy')
			div class :{{hierarchy.classHierarchy}}
		div(ng-show='hierarchy.order')
			div order :{{hierarchy.order}}
		div(ng-show='hierarchy.family')
			div family :{{hierarchy.family}}
		div(ng-show='hierarchy.genus')
			div genus :{{hierarchy.genus}}
		div(ng-show='hierarchy.subGenus')
			div subGenus :{{hierarchy.subGenus}}
		//
			<div ng-show="hierarchy.taxonRank">
			<div>taxonRank :{{hierarchy.taxonRank}}</div>
			</div>
		div(ng-show='hierarchy.specificEpithet')
			div specificEpithet :{{hierarchy.specificEpithet}}
		div(ng-show='hierarchy.infraspecificEpithet')
			div infraspecificEpithet :{{hierarchy.infraspecificEpithet}}
		div(ng-show='hierarchy.higherClassification')
			div higherClassification :{{hierarchy.higherClassification}}
		//
			<div ng-show="hierarchy.parentTaxon">
			<div>parentTaxon :{{hierarchy.parentTaxon}}</div>
			</div>
		div(ng-show='hierarchy.ancillaryData[0].source')
			div Source :{{hierarchy.ancillaryData[0].source}}
		hr
	//
		<div ng-controller="CollapseDemoCtrl">
		<button class="btn btn-default" ng-click="isCollapsed = !isCollapsed">Add Hierarchy</button>
		<hr>
		<div collapse="isCollapsed">
		<div class="form-group">
		<h4>Classification:</h4>
		<input type="text"  ng-model="hierarchy.classification"/>
		<br>
		<h4>Recommended:</h4>
		<input type="text"  ng-model="hierarchy.recommended"/>
		<br>
		<h4>Kingdom:</h4>
		<input type="text"  ng-model="hierarchy.kingdom"/>
		<br>
		<h4>Phylum:</h4>
		<input type="text"  ng-model="hierarchy.phylum"/>
		<br>
		<h4>Class:</h4>
		<input type="text"  ng-model="hierarchy.classHierarchy"/>
		<br>
		<h4>Order:</h4>
		<input type="text"  ng-model="hierarchy.order"/>
		<br>
		<h4>Family:</h4>
		<input type="text"  ng-model="hierarchy.family"/>
		<br>
		<h4>Genus:</h4>
		<input type="text"  ng-model="hierarchy.genus"/>
		<br>
		<h4>Sub Genus:</h4>
		<input type="text"  ng-model="hierarchy.subGenus"/>
		<br>
		<h4>Taxon Rank:</h4>
		<input type="text"  ng-model="hierarchy.taxonRank"/>
		<br>
		<h4>Specific Epithet:</h4>
		<input type="text"  ng-model="hierarchy.specificEpithet"/>
		<br>
		<h4>Infraspecific Epithet:</h4>
		<input type="text"  ng-model="hierarchy.infraspecificEpithet"/>
		<br>
		<h4>Higher Classification:</h4>
		<input type="text"  ng-model="hierarchy.higherClassification"/>
		<br>
		<h4>Parent Taxon:</h4>
		<input type="text"  ng-model="hierarchy.parentTaxon"/>
		<br>
		<h4>Ancillary Data:</h4>
		<button type="button" class="btn btn-info btn-sm" ng-click="addAncillaryData(hierarchy.ancillaryData)">Ancillary Data<span class="glyphicon glyphicon-plus"></span> </button>
		<div ng-repeat="ancillary in hierarchy.ancillaryData">
		<div class="form-group">
		<h4>License:</h4>
		<input type="text"  ng-model="ancillary.license"/>
		<hr>
		<h4>Rights Holder:</h4>
		<input type="text"  ng-model="ancillary.rightsHolder"/>
		<hr>
		<h4>Bibliographic Citation:</h4>
		<input type="text"  ng-model="ancillary.bibliographicCitation"/>
		<hr>
		<h4>Source:</h4>
		<input type="text"  ng-model="ancillary.source"/>
		<hr>
		<h4>Media URL:</h4>
		<input type="text"  ng-model="ancillary.mediaURL"/>
		<hr>
		<h4>References:</h4>
		<div ng-repeat="ref in ancillary.reference">
		<h5>Identifier:</h5>
		<input type="text"  ng-model="ref.identifier"/>
		<br>
		<h5>Datatype:</h5>
		<input type="text"  ng-model="ref.datatype"/>
		<br>
		<h5>Source:</h5>
		<input type="text"  ng-model="ref.source"/>
		<hr>
		</div>
		<button type="button" class="btn btn-info btn-sm" ng-click="addReference(ancillary.reference)">Add Reference<span class="glyphicon glyphicon-plus"></span> </button>
		<hr>
		</div>
		</div>
		<br>
		<button type="button" class="btn btn-info btn-sm" ng-click="addHierarchy(formData.hierarchy,hierarchy)">Accept</button>
		</div>
		</div>
		</div>
		<br>
.col-xs-6.col-xs-offset-3
	a.btn.btn-block.btn-info(ui-sref='form.taxoDesc')
		| Next Section
		span.glyphicon.glyphicon-circle-arrow-right

				input(type='text', data-ng-model='formData.dataset.pubDate')
				h4 language:
				input(type='text', data-ng-model='formData.dataset.language')
				h4 abstract:
				input(type='text', data-ng-model='formData.dataset.abstract')
				h4 keyworset:
				input(type='text', data-ng-model='formData.dataset.keyworset')
				h4 additionalInfo:
				input(type='text', data-ng-model='formData.dataset.additionalInfo')
				h4 intellectialRights:
				input(type='text', data-ng-model='formData.dataset.intellectialRights')
				h4 distribution:
				input(type='text', data-ng-model='formData.dataset.distribution')
				h4 coverage:
				input(type='text', data-ng-model='formData.dataset.coverage')
				h4 contact:
				input(type='text', data-ng-model='formData.dataset.contact')
				h4 methods:
				input(type='text', data-ng-model='formData.dataset.methods')
				h4 project:
				input(type='text', data-ng-model='formData.dataset.project')
	h3(translate='') simplePliCTerm.name.resourceLogoUrl
	hr
	input(type='text', name='resourceLogoUrl', data-ng-model='formData.resourceLogoUrl')
	br
	h3(translate='') simplePliCTerm.name.References
	hr
	input(type='text', name='reference', data-ng-model='formData.reference')
	br

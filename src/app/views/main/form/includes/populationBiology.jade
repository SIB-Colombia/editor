div(ng-controller='PopulationBiologyCtrl')
	.panel.panel-default
		.panel-heading
			spant
				| {{"simplePliCTerm.name.PopulationBiology"|translate}}
				spant
					a.btn-xs(data-container='body', data-toggle='popover', data-placement='left', title='{{"simplePliCTerm.definition.PopulationBiology"|translate}}')
						span.glyphicon.glyphicon-info-sign
			//button.btn.btn-success.btn-xs.pull-right(type='button', ng-click='addPopulationBiology()') 
				span(class="glyphicon glyphicon-floppy-disk", aria-hidden='true')
		.panel-body
			div.table-responsive(ng-show='(formData.populationBiology.populationBiologyAtomized).length')
				table.table.table-bordered
					thead
						tr
							th {{"simplePliCTerm.name.Region"|translate}}
							th {{"simplePliCTerm.name.AbundanceData"|translate}}
							th {{"simplePliCTerm.name.DensityData"|translate}}
							th {{"simplePliCTerm.name.PatternDistrubution"|translate}}
							th
					
					tbody
						tr(ng-repeat="populationBiologyAtomized in formData.populationBiology.populationBiologyAtomized track by $index")
							td
								| {{populationBiologyAtomized.region.measurementValue}}
							td
								| {{populationBiologyAtomized.abundanceData.measurementValue}}	
							td
								| {{populationBiologyAtomized.densityData.measurementValue}}
							td
								| {{populationBiologyAtomized.patternDistribution.measurementValue}}	
							td
								button.btn(ng-click='removePopulation(formData.populationBiology.populationBiologyAtomized,populationBiologyAtomized)')
									span(class="glyphicon glyphicon-trash", aria-hidden='true')
								button.btn(ng-click='editPopulation(formData.populationBiology.populationBiologyAtomized,populationBiologyAtomized)')
									span(class="glyphicon glyphicon-pencil", aria-hidden='true')
			.form-group

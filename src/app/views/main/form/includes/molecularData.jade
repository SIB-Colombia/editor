.panel.panel-default
	.panel-heading
		spant
			| {{"simplePliCTerm.name.MolecularData" | translate}}
			spant
			a.btn-xs(data-container='body', data-toggle='popover', data-placement='left', title='{{"simplePliCTerm.definition.MolecularData"|translate}}')
				span.glyphicon.glyphicon-info-sign
	.panel-body
		.selected-item(ng-repeat='measurementOrFact in formData.molecularData.molecularDataAtomized.measurementOrFact')
			div(ng-show='measurementOrFact')
			div {{measurementOrFact}}
		.form-group
			div(ng-controller='MeasurementCtrl')
			select(ng-model='molecularDataAtomizedType.measurementOrFact.measurementType', ng-options="attr.measurementtype as attr.measurementtype for attr in molecularDatas | unique:'measurementtype'")
				option(value='') - Seleccione -
			select(ng-model='molecularDataAtomizedType.measurementOrFact.measurementValue', ng-disabled='!molecularDataAtomizedType.measurementOrFact.measurementType', ng-options='item.measurementvalue as item.measurementvalue for item in selectedAttr ')
				option(value='') - Seleccione -
		.form-group
			label.col-xs-2.control-label(for='relatedTo', translate='') simplePliCTerm.name.Related to
			.col-xs-10
			textarea(rows='3', name='relatedTo', ng-model='molecularDataAtomizedType.relatedTo')
		button.btn.btn-success.btn-sm.pull-right(type='button', ng-click='addMolecularDataAtomizedType(formData.molecularData.molecularDataAtomized,molecularDataAtomizedType)') Guardar
		.form-group
			hr
			label(for='molecularDataUnstructured', translate='') simplePliCTerm.name.MolecularDataUnstructured
			em (opcional)
			a.btn-xs(data-container='body', data-toggle='popover', data-placement='left', title='{{"simplePliCTerm.definition.MolecularDataUnstructured"|translate}}')
			span.glyphicon.glyphicon-info-sign
			textarea(rows='3', name='molecularData', ng-model='formData.molecularData.molecularDataUnstructured')
			div(ng-repeat='ancillary in formData.molecularData.ancillaryData')
			| {{ancillary.license}} - {{ancillary.rights}} - {{ancillary.mediaURL}}
			div(ng-controller='CollapseDemoCtrl')
				.form-group
					button.btn.btn-primary.btn-sm.pull-right(type='button', ng-click='isCollapsed = !isCollapsed') {{"simplePliCTerm.name.AncillaryData"|translate}}
					.col-xs-10
					div(collapse='isCollapsed')
						input(type='text', ng-model='ancillaryData.license', placeholder='{{"simplePliCTerm.name.license"|translate}}')
						input(type='text', ng-model='ancillaryData.rights', placeholder='{{"simplePliCTerm.name.rights"|translate}}')
						input(type='text', ng-model='ancillaryData.bibliographicCitation', placeholder='{{"simplePliCTerm.name.bibliographicCitation"|translate}}')
						input(type='text', ng-model='ancillaryData.source', placeholder='{{"simplePliCTerm.name.source"|translate}}')
						input(type='text', ng-model='ancillaryData.mediaURL', placeholder='{{"simplePliCTerm.name.mediaURL"|translate}}')
						button.btn.btn-primary.btn-sm(type='button', ng-click='addAncillaryData(formData.molecularData.ancillaryData,ancillaryData)', translate='')
						| Guardar
						span.glyphicon.glyphicon-plus
